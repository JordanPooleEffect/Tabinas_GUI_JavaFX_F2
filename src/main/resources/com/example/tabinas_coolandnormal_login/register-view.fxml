<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tabinas_coolandnormal_login.RegisterController">
    <children>
        <Label text="Username:" />
        <TextField fx:id="usernameField" />
        <Label text="Password:" />
        <PasswordField fx:id="passwordField" />
        <Button fx:id="registerButton" text="Register" onAction="#handleRegister" />
        <Button fx:id="loginBtn" text="Go To Login Page" onAction="#handleLogin" layoutX="200" layoutY="200" />
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>
    <spacing>10</spacing>
</VBox>
