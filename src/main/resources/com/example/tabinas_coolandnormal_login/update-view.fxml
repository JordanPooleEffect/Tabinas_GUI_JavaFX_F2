<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tabinas_coolandnormal_login.UpdateController"
            prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="btnSumbit" text="Submit" onAction="#handleSubmit" layoutX="100" layoutY="200"/>
    <Button fx:id="btnDashboard" text="Back to Dashboard" onAction="#handleDashboard" layoutX="200" layoutY="200"/>

    <GridPane prefHeight="134.0" prefWidth="262.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Label alignment="CENTER" text="Current Username:">
                <font>
                    <Font size="20.0" />
                </font>
            </Label>
            <Label text="New Password:" GridPane.rowIndex="1">
                <font>
                    <Font size="20.0" />
                </font>
            </Label>
            <TextField fx:id="fieldNewUsername" prefHeight="41.0" prefWidth="130.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="fieldNewPassword" prefHeight="40.0" prefWidth="151.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        </children>
    </GridPane>
</AnchorPane>
